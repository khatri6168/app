  pod 'Firebase/RemoteConfig'
  pod 'OneSignal', '>= 2.6.2', '< 3.0'
  pod 'Kitemetrics'
  pod 'Firebase/Core'
  pod 'Fabric', '~> 1.7.7'
  pod 'Crashlytics', '~> 3.10.2'

end
